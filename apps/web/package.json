{
  "name": "@apps/web",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview --host",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check --ignore-path=../../.prettierignore . && eslint --ignore-path=../../.eslintignore .",
    "format": "prettier --write --ignore-path=../../.prettierignore .",
    "docker:run": "docker run -p 3080:3000 koakh/koakh-sveltekit-payloadcms-trpc-turborepo-starter-server"
  },
  "devDependencies": {
    "@apps/server": "workspace:*",
    "@iconify/json": "^2.2.96",
    "@sveltejs/adapter-auto": "^2.1.0",
    "@sveltejs/adapter-node": "^1.3.1",
    "@sveltejs/kit": "^1.22.4",
    "@unocss/extractor-svelte": "^0.54.1",
    "@unocss/preset-mini": "^0.54.1",
    "@unocss/reset": "^0.54.1",
    "eslint-config-custom": "workspace:*",
    "svelte": "^3.59.1",
    "svelte-check": "^3.4.6",
    "tslib": "^2.6.1",
    "typescript": "^5.1.6",
    "unocss": "^0.54.1",
    "vite": "^4.4.8"
  },
  "dependencies": {
    "@trpc/client": "^10.36.0",
    "clsx": "^2.0.0",
